<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/przTydzien"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/przNazwa"
        android:layout_width="200dp"
        android:layout_height="30dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="11dp"
        android:layout_marginStart="11dp"
        android:layout_marginTop="39dp"
        android:text="przNazwa" />

    <TextView
        android:id="@+id/przGodzinaOd"
        android:layout_width="60dp"
        android:layout_height="30dp"
        android:layout_alignTop="@+id/przSala"
        android:layout_toEndOf="@+id/przNazwa"
        android:layout_toRightOf="@+id/przNazwa"
        android:text="przGodzina" />

    <TextView
        android:id="@+id/przSala"
        android:layout_width="60dp"
        android:layout_height="30dp"
        android:layout_alignLeft="@+id/przNazwa"
        android:layout_alignStart="@+id/przNazwa"
        android:layout_below="@+id/przNazwa"
        android:layout_marginTop="37dp"
        android:text="przSala" />

    <TextView
        android:id="@+id/przGodzDo"
        android:layout_width="60dp"
        android:layout_height="30dp"
        android:layout_alignBaseline="@+id/przGodzinaOd"
        android:layout_alignBottom="@+id/przGodzinaOd"
        android:layout_marginLeft="11dp"
        android:layout_marginStart="11dp"
        android:layout_toEndOf="@+id/przGodzinaOd"
        android:layout_toRightOf="@+id/przGodzinaOd"
        android:text="przGodzDo" />

</RelativeLayout>